{
  "name": "thght-backend",
  "version": "1.0.0",
  "main": "app.js",
  "private": false,
  "scripts": {
    "build": "tsc",
    "clean": "rimraf ./dist",
    "develop": "ts-node-dev --no-notify --respawn --transpileOnly --ignore-watch node_modules src/app.ts",
    "fix": "npm run lint:js -- --fix",
    "format": "prettier --write '**/*.{css,html,js,jsx,json,md,ts,tsx,yml,yaml}'",
    "lint": "eslint '**/*.{js,jsx,ts,tsx}'",
    "prebuild": "npm run clean",
    "start": "node dist/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tderleth/thght-backend.git"
  },
  "bugs": {
    "url": "https://github.com/tderleth/thght-backend/issues"
  },
  "homepage": "https://github.com/tderleth/thght-backend#readme",
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.1",
    "@types/node": "^12.12.8",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.6.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.1.0",
    "prettier": "^1.17.1",
    "pretty-quick": "^2.0.0",
    "rimraf": "^3.0.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.6.4"
  }
}
