{
  "name": "thght-backend",
  "version": "1.0.0",
  "main": "app.js",
  "private": false,
  "scripts": {
    "build": "tsc",
    "clean": "rimraf ./dist",
    "develop": "ts-node-dev --no-notify --respawn --transpileOnly --ignore-watch node_modules src/app.ts",
    "fix": "npm run lint -- --fix",
    "format": "prettier --write '**/*.{css,html,js,jsx,json,md,ts,tsx,yml,yaml}'",
    "lint": "eslint '**/*.{js,jsx,ts,tsx}'",
    "prebuild": "npm run clean",
    "start": "node dist/app.js",
    "test": "jest --colors --runInBand --verbose --silent --forceExit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tderleth/thght-backend.git"
  },
  "bugs": {
    "url": "https://github.com/tderleth/thght-backend/issues"
  },
  "homepage": "https://github.com/tderleth/thght-backend#readme",
  "dependencies": {
    "@types/helmet": "0.0.45",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.21.2",
    "rimraf": "^3.0.0",
    "swagger-ui-express": "^4.1.2"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.14",
    "@types/supertest": "^2.0.8",
    "@types/swagger-ui-express": "^4.1.0",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "aws-cdk": "^1.18.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "prettier": "^1.17.1",
    "pretty-quick": "^2.0.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.2.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.6.4"
  }
}
